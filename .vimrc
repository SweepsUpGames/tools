set nocompatible              " be iMproved, required
filetype off                  " required

" highlighting
set relativenumber
syntax on
set background=dark
set cursorline

" tabbing
set expandtab
set shiftwidth=2
set softtabstop=2

" folding settings
set foldmethod=indent
set foldnestmax=10
set nofoldenable
set foldlevel=1

let g:solarized_termcolors=256
